{
  "expo": {
    "name": "WebStol",
    "slug": "web-stol",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logoSquareWhite.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#88D3EE"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.WebStol.WebStol"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoSquareWhite.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.WebStol.WebStol"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
